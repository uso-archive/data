  "css": "@-moz-document url(\"https://www.rocketbeans.tv/superchad/\"), url-prefix(\"https://rocketbeans.tv/superchad/\") {\r\n:root {\r\n--b0: /*[[b0]]*/;\r\n--b1: /*[[b1]]*/;\r\n--b2: /*[[b2]]*/;\r\n--b3: /*[[b3]]*/;\r\n--o1: /*[[o1]]*/;\r\n}\r\n.chat-container .chatSelector h1,.chat-container li .message .inlineMessage{color:var(--o1)}.chat-container .chatSelector{background-color:var(--b3)}.chat-container li:nth-child(odd){background-color:var(--b1)}.chat-container{background-color:var(--b2)}.chat-container ::-webkit-scrollbar-thumb{background-color:var(--o1);border:2px solid var(--o1)}.chat-container ::-webkit-scrollbar-track{background-color:var(--b2)}.chat-container li:hover{background-color:var(--o1)!important}.chat-container li:hover .message .username[data-username]{color:var(--b0)}.chat-container li:hover .message .inlineMessage{color:var(--b1)}.btn:focus,.btn:hover{background-color:var(--o1);color:var(--b0)}.btn{background-color:var(--b3)}\r\n}",
