  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document domain(\"www.baidu.com\") {\r\n\r\n.s-weather-border{\r\n    border-bottom: none !important;\r\n}\r\n.bottom-line1,.bottom-line2,#s_seth{\r\n    display: none !important;\r\n}\r\n.s-skin-user{\r\n    background: url(/*[[bgimg]]*/) no-repeat center fixed;\r\n    background-size: 100% auto;\r\n}\r\n#lg img{\r\n    width: 0 !important;\r\n}\r\n#lg{\r\n    background: url(/*[[logo]]*/) no-repeat center !important;\r\n}\r\n#kw{\r\n    background: transparent !important;\r\n    background-color: /*[[inputbg]]*/ !important;\r\n    border-color: /*[[inputcolor]]*/ !important;\r\n    -moz-transition: background-color .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out;\r\n}\r\n#kw:focus{\r\n    /*[[kw]]*/\r\n    background-color: /*[[inputbgf]]*/ !important;\r\n    -moz-transition: background-color .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out;\r\n}\r\n.btn_wr{\r\n    background: none !important;\r\n}\r\n.btn{\r\n    /*[[btcolor]]*/\r\n    width: 80px !important;\r\n    height: 28px !important;\r\n    margin-top: 2px !important;\r\n    background: -moz-linear-gradient(left,rgba(255,255,255,.2),rgba(255,255,255,.4),rgba(255,255,255,.2)) !important; \r\n}\r\n.btn:hover{\r\n    background: -moz-linear-gradient(left,rgba(255,255,255,.3),rgba(255,255,255,.5),rgba(255,255,255,.3)) !important;\r\n}\r\n.s-title .s-title-bg,.s-content{\r\n    background: transparent !important;\r\n}\r\n.mod-sc.s-mod{\r\n    border: none !important;\r\n    background-color: /*[[opacity]]*/ !important;\r\n    -moz-transition: background-color .5s ease-in-out,box-shadow .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out,box-shadow .5s ease-in-out;\r\n}\r\n.mod-sc.s-mod:hover{\r\n    background-color: /*[[hopacity]]*/ !important;\r\n    box-shadow: 2px 2px 2px 1px rgba(0,0,0,.1) !important;\r\n    -moz-transition: background-color .5s ease-in-out,box-shadow .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out,box-shadow .5s ease-in-out;\r\n}\r\n.s-nplus *{\r\n    border: none !important;\r\n}\r\n/*[[inputfocus]]*/\r\n/*[[linkcolor]]*/\r\n.s-mod-nav,.s-ncf-name-link{\r\n    color: #42474A !important;\r\n}\r\n.s-nav-name img{\r\n    display: none !important;\r\n}\r\n.mod-sc.s-mod:hover .s-title:before{\r\n    content: url(/*[[moe]]*/) !important;\r\n}\r\n/*[[linkhover]]*/\r\n.user-name-top{\r\n    background: none !important;\r\n}\r\n.s-username-menu,.s-username-menu a.sep{\r\n    border-color: rgba(255,255,255,.3) !important;\r\n}\r\n.s-username-menu a{\r\n    background-color: rgba(255,255,255,.4) !important;\r\n    -moz-transition: background-color .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out;\r\n}\r\n.s-username-menu a:hover{\r\n    background-color: rgba(255,255,255,.6) !important;\r\n    -moz-transition: background-color .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out;\r\n}\r\n#st{\r\n    background-color: rgba(255,255,255,.4) !important;\r\n}\r\ndiv[id^=\"sd_\"]{\r\n    border-color: rgba(255,255,255,.4) !important;\r\n}\r\n#st tr{\r\n    background-color: rgba(255,255,255,.4) !important;\r\n    -moz-transition: background-color .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out;\r\n}\r\n#st tr:hover{\r\n    background-color: rgba(255,255,255,.6) !important;\r\n    -moz-transition: background-color .5s ease-in-out;\r\n    -webkit-transition: background-color .5s ease-in-out;\r\n}\r\n#st tr b{\r\n    -moz-transition: color .5s ease-in-out;\r\n    -webkit-transition: color .5s ease-in-out;\r\n}\r\n#st tr:hover b{\r\n    color: red !important;\r\n    -moz-transition: color .5s ease-in-out;\r\n    -webkit-transition: color .5s ease-in-out;\r\n}\r\n/*[[layout]]*/",
