  "css": "@-moz-document domain(\"lurkmore.to\"){\r\n.mw-body {\r\nmax-width:/*[[mw]]*/ !important;\r\n}\r\ndiv#mw-panel{width:12.5rem !important}\r\n.mw-body{margin-left: 13rem !important}\r\n#left-navigation {\r\n    margin-left: 13rem !important;\r\n}\r\n/* ESI 2.5 core */\r\nhtml {font-size: /*[[fs]]*/ !important; line-height: /*[[lh]]*/ !important; }\r\nesi-widest-text-block {max-width: /*[[mw]]*/ !important}\r\na {text-decoration: none !important}\r\nesi-text-block {width: x rem; height: y rem}\t\r\n* {font-size: inherit !important; line-height: inherit !important;font-family:/*[[ff]]*/ !important}\r\nh1 {font-size: 1.7 rem !important; font-size: calc(1.4375rem + 8px) !important; line-height: calc(1.4375rem * 1.052 + 8px * 1.052 + 5px) !important}\r\nh2 {font-size: 1.4 rem !important; font-size: calc(1.25rem + 4px) !important; line-height: calc(1.25rem * 1.052 + 1.052 + 4px + 5px) !important}\r\nh3 {font-size: 1.17 rem !important; font-size: calc(1.0625rem + 2px) !important; line-height: calc(1.0625rem * 1.052 + 2px * 1.052 + 5px) !important}\r\nsup, sub{line-height: 1 !important; font-size: 0.6875rem; font-size: calc(1rem - 5px) !important;}\r\nh1,h2,h3{padding-bottom: 1rem !important}\r\n#coordinates{display:none !important}\r\n}\r\n",
