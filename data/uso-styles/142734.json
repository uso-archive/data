us-card {\r\n    color: var(--hi-3);\r\n    background-color: var(--bg-2);\r\n    border-color: var(--hi-2);\r\n}\r\n.status-card:hover {\r\n    background-color: var(--bg-3);\r\n}\r\n.status-card__image {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n    background-color: var(--bg-0);\r\n}\r\n/* リアクションボタンランチャー */\r\n\r\n.detailed-status__action-bar {\r\n    background-color: var(--bg-3);\r\n    border-top-color: var(--bg-1);\r\n    border-bottom-color: var(--bg-1);\r\n}\r\n.icon-button.disabled {\r\n    color: var(--hi-2);\r\n}\r\n/*\r\n  ========================================\r\n  プロフィールカラム\r\n  ========================================\r\n*/\r\n\r\n.account__header>div {\r\n    background-color: transparent;\r\n    background-image: linear-gradient(rgba(0, 0, 16, 0.8), rgba(8, 12, 32, 0.6));\r\n}\r\n.account__action-bar {\r\n    border-top-color: transparent;\r\n    border-bottom-color: #080F18;\r\n}\r\n.account__action-bar__tab {\r\n    border-left-color: transparent;\r\n}\r\n/* メディア */\r\n\r\n.account-section-headline {\r\n    background-color: transparent;\r\n    background-image: linear-gradient(rgba(0, 0, 0, 0), var(--panel-color-2));\r\n    color: var(--hi-3);\r\n    border-bottom-color: var(--bg-0);\r\n}\r\n.account-section-headline:before,\r\n.account-section-headline:after {\r\n    border-color: transparent transparent var(--bg-0);\r\n}\r\n/* ギャラリー */\r\n\r\n.account-gallery__container {\r\n    background-image: linear-gradient(var(--bg-0), var(--panel-color-2));\r\n}\r\n/* 通知設定 */\r\n/* トグルスイッチ */\r\n\r\n.react-toggle-track {\r\n    background-color: var(--bg-1);\r\n}\r\n.react-toggle:hover:not(.react-toggle--disabled) .react-toggle-track {\r\n    background-color: var(--hi-2);\r\n}\r\n/*\r\n  ========================================\r\n  スタートメニューカラム (../web/getting-started)\r\n  ========================================\r\n*/\r\n\r\n.getting-started__wrapper {\r\n    background-color: var(--hi-0);\r\n    background: linear-gradient(var(--bg-1), var(--panel-color-2));\r\n}\r\n/*\r\n  スタートメニュー\r\n  ----------------------------------------\r\n*/\r\n\r\n.column-subheading,\r\n.column-link {\r\n    background: transparent;\r\n}\r\n.column-subheading {\r\n    color: var(--hi-3);\r\n}\r\n.column-link {\r\n    color: var(--fg-2);\r\n}\r\n.column-link:hover {\r\n    background-color: var(--hi-2);\r\n    color: #FFFFFF;\r\n}\r\n/*\r\n  マストドンの説明文\r\n  ----------------------------------------\r\n*/\r\n\r\n.getting-started p {\r\n    color: var(--fg-3);\r\n}\r\n.getting-started a {\r\n    color: var(--link-color);\r\n}\r\n/*\r\n  スクロールバー\r\n  ----------------------------------------\r\n*/\r\n\r\n::-webkit-scrollbar-thumb {\r\n    background-color: var(--hi-0);\r\n}\r\n::-webkit-scrollbar-thumb:hover {\r\n    background-color: var(--hi-2);\r\n}\r\n::-webkit-scrollbar-track:hover {\r\n    background-color: var(--bg-0);\r\n}\r\n}"}