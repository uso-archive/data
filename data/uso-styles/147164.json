  "css": "@-moz-document url-prefix(\"https://outlook.live.com/mail/\") {\r\n/* Right-side ad banner */\r\ndiv#app > div > div:nth-child(3) > div > div > div:nth-child(4) {\r\n    display: none !important;\r\n}\r\n\r\n/* Bottom ad banner */\r\ndiv#app > div > div:nth-child(3) > div > div:nth-child(2) {\r\n    display: none !important;\r\n}\r\n\r\n/* \"Upgrade\" banner */\r\ndiv#app > div > div:nth-child(3) > div > div:nth-child(1) > div:nth-child(1) > div:nth-child(2) > div:nth-child(1) > div:nth-child(2) {\r\n    display: none !important;\r\n}\r\n}",
