  "css": "/*\r\n*************************************************\r\n*Автор:      Vidoc300                           *\r\n*Версия:     last.fm beta v1.6                  *\r\n*URL:        https://userstyles.org/users/285652*\r\n*@Все права защищены! :)                        *\r\n*************************************************\r\n*/\r\n\r\n@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document domain(\"last.fm\") {\r\n  html, body {background: url(/*[[Images]]*/) no-repeat fixed !important; -webkit-background-size: cover !important; background-size: cover !important; color: /*[[text_color]]*/ !important;}\r\n  body::before {background: none !important;}\r\n  a {color: /*[[Color_lnk]]*/ !important;}\r\n  h1, .h1, h3, .h3, h4, .h4 {color: /*[[text_color]]*/ !important;}\r\n  /*Заголовок*/\r\n  .header {color: /*[[text_color]]*/ !important;}\r\n  .header-scrobble-since {color: /*[[text_color2]]*/ !important;}\r\n  .metadata-title {color: /*[[text_color2]]*/ !important;}\r\n  .metadata-display {color: /*[[text_color2]]*/ !important;}\r\n  .nag-bar, .alert:first-child {/*[[Sys]]*/}\r\n  .two-column-layout .content-top {background: none !important;}\r\n  .header .header-background {/*[[header]]*/}\r\n  .player-bar {/*[[play]]*/}\r\n  .top-bar {background: none !important;}\r\n  .header {background: url(/*[[shapka]]*/) no-repeat; background-size: 100% 100%;}\r\n  .dropdown:hover .dropdown-menu, .dropdown:focus .dropdown-menu, .dropdown .dropdown-menu-item:hover, .dropdown .dropdown-menu-item:focus, .dropdown-menu.dropdown--open {background: /*[[menu_fon_color]]*/ !important;}\r\n  /*Поиск*/\r\n  .masthead .search-form .search-field {border-color: /*[[search]]*/ !important; color: /*[[search]]*/ !important;}\r\n   /********************/\r\n  /*Контент*/\r\n  .container {background: none !important;}\r\n  .two-column-layout .content-top .container {background: none !important;}\r\n  .content-top::after, .content-top .content-top-lower::after {border: none !important;}\r\n  .stationlinks {color: /*[[Color_lnk]]*/ !important;}\r\n  .stationlink-list li {box-shadow: none !important;}\r\n  .chartlist tr:nth-child(2n) td {background: none !important;}\r\n  .js-link-block {box-shadow: none !important;}\r\n  .chartlist td, .chartlist th {box-shadow: none !important;}\r\n  .comments .media .text-container .comment-text {color: white !important;}\r\n  .chartlist .countbar-bar-slug {background-color: /*[[menu_fon_color]]*/ !important;}\r\n  .recs-feed .context {background: none !important; box-shadow: none !important;}\r\n  .chartlist .now-scrobbling {/*[[scrobl]]*/}\r\n  .avatar {border-radius: /*[[radius]]*/%;}\r\n  .feedback-message {/*[[feedback]]*/}\r\n  .chartlist .chartlist-index, .chartlist .chartlist-artists, .chartlist .chartlist-album, .chartlist .artist-name-spacer, .chartlist .chartlist-duration, .chartlist .chartlist-timestamp, .chartlist .chartlist-countbar, .comments .media .text-container .timestamp {color: /*[[time_color]]*/ !important;}\r\n  .grid-items-cover-image {background-image: linear-gradient(to bottom, transparent 25%, /*[[podsvetka]]*/ 100%);}\r\n  .footer-wrapper .container .footer {color: /*[[text_color]]*/ !important;}\r\n  /********************/\r\n  /*Плеер*/\r\n  .player-bar.inactive .player-bar-btn {opacity: 1 !important;}\r\n  .player-bar .player-bar-progress {background: /*[[menu_fon_color]]*/ !important;}\r\n  /*[[player_buttons]]*/\r\n  /**/\r\n  /*Виджет*/\r\n  .kerve.widget {background: none !important; border: none !important;}\r\n  .yad-widget li {border: none !important; border-bottom: 0px !important;}\r\n  .features-promoted .features-post-title {color: /*[[text_color]]*/ !important;}\r\n  /********************/\r\n  /*Сообщения*/\r\n  .inbox .inbox-message--unviewed {background: none !important; border: none !important;}\r\n  .inbox .inbox-buttons {border: none !important;}\r\n  .inbox tr, .inbox td {box-shadow: none !important;}\r\n  /********************/\r\n  /**/\r\n  .comments .media .text-container .comment-text {color: /*[[comm_color]]*/ !important;}\r\n  /********************/\r\n  /*Настройки*/\r\n  .section-settings {background: none !important; border: none !important;}\r\n  .section-settings-toggle {border: none !important;}\r\n  .selectbox .selectboxit-btn {color: /*[[Color_lnk]]*/ !important;}\r\n  .selectbox .selectboxit-options {background: /*[[menu_fon_color]]*/ !important; border: none !important;}\r\n  .selectbox .selectboxit-focus .selectboxit-option-anchor {background: /*[[menu_fon_color]]*/ !important;}\r\n  /**/\r\n  /*События*/\r\n  .events-filters {box-shadow: none !important;}\r\n  .events-list, .events-list-item {border: none !important;}\r\n  .events-list td, .events-list th {box-shadow: none !important;}\r\n  /********************/\r\n  /*Музыка*/\r\n  .globalchart .globalchart-item:first-child {background: none !important;}\r\n  .globalchart {z-index: auto !important;}\r\n  .two-column-layout .content-top, .two-column-layout .content-top .container, .two-column-layout .container {background: none !important;}\r\n  .container {background: none !important;}\r\n  .globalchart .globalchart-item:first-child {background: none !important;}\r\n  /********************/\r\n  /*Библиотека*/\r\n  div.graph-container {display: none !important;}\r\n  .scrobbles-layout .fluid-container {background: none !important;}\r\n  .scrobbles-layout .overlay-control-bar, .scrobbles-layout .overlay-content {border: none !important;}\r\n  .scrobbles-layout .overlay-side-rail {background: none !important;}\r\n  .scrobbles-layout .scrobbles-nav-bar .secondary-nav {background: none !important;}\r\n  .scrobbles-layout .scrobbles-nav-bar .secondary-nav-item-link.secondary-nav-item-link--active {background: black !important;}\r\n  .scrobbles-layout .overlay-control-bar::after {border: none !important;}\r\n  /**/\r\n  /*Подвал*/\r\n  .footer-wrapper {background: none !important;}\r\n  .footer-wrapper .container .footer .site-links {box-shadow: none !important;}\r\n  /********************/\r\n  /*Реклама*/\r\n  .related-ads {display: none !important;}\r\n  /********************/\r\n  .yad-lightbox, .yad-lightbox-content {background: none !important;}\r\n  /*******************/\r\n  .main-content::before {background: none !important;}\r\n  /*[[youtube]]*/\r\n}\r\n@-moz-document domain(\"yimg.com\") {\r\n  .yad-widget li {border: none !important;}\r\n  .yad-widget .yad-title.C {color: white !important;}\r\n  .yad-widget .yad-sponsored {background: none !important;}\r\n  /********************/\r\n}",
