  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document domain(\"vk.com\") {\r\n\r\n#im_peer_controls .emoji_tt_wrap, #mbe_emoji_wrap .emoji_tt_wrap, #imw_emoji_wrap .emoji_tt_wrap, .emoji_tooltip_new {width: 478px !important}\r\n#im_peer_controls .emoji_list, #mbe_emoji_wrap .emoji_list, #imw_emoji_wrap .emoji_list, .emoji_tooltip_new .emoji_list {width: 460px !important}\r\n#im_peer_controls .emoji_sticker_item, #mbe_emoji_wrap .emoji_sticker_item, #imw_emoji_wrap .emoji_sticker_item {width: 64px !important; padding-left: .5px !important; padding-right: .5px !important}\r\n/*[[size]]*/\r\n#im_peer_controls .emoji_tt_wrap {right: 82px !important}\r\n#mbe_emoji_wrap .emoji_tt_wrap {right: -14px !important}\r\n#imw_emoji_wrap .emoji_tt_wrap, .emoji_tooltip_new {right: -27px !important}\r\n\r\n#im_peer_controls .emoji_pointer {left: 398px !important}\r\n#mbe_emoji_wrap .emoji_pointer {left: 442px !important}\r\n#imw_emoji_wrap .emoji_pointer, .emoji_tooltip_new .emoji_pointer {left: 431px !important}\r\n\r\n.emoji_expand_shadow, .emoji_expanded .emoji_expand_shadow, .emoji_expanded .emoji_expand_shadow_top {display: none !important}\r\n\r\n}",
