  "css": "@-moz-document domain('rainbowdash.net') {\r\n#content { \r\n\twidth: 70%;\r\n}\r\n#wrap { \r\n\twidth: 95%; \r\n\tmax-width: 1300px; \r\n}\r\n#aside_primary { \r\n\twidth: 20%;\r\n}\r\ndiv#popular_notices.section ol.notices.xoxo li.hentry.notice { \r\n\t//display: none; \r\n\tline-height: 1.3 !important;\r\n\tfont-size: 0.8em !important;\r\n}\r\ndiv#featured_users.section { \r\n\tdisplay: none; \r\n}\r\n.location { \r\n\tdisplay: /*[[location]]*/; \r\n}\r\n\r\n/*[[ShrinkAvatar]]*/ \r\nimg[width='96'].avatar {\r\n        height: 96px; \r\n        width: 96px; \r\n}\r\n\r\n.notice { \r\n        padding-top: 5px;\r\n        padding-bottom: 5px; \r\n}\r\n.notice div.entry-content { \r\n        font-size: 0.75em; \r\n}\r\n/*[[CompactMenu]]*/\r\ndiv#content h1:first-child { \r\n        display: none; \r\n}\r\nol.notices.xoxo .notice:first-child { \r\n        border-top-width: 0px; \r\n}\r\n}",
