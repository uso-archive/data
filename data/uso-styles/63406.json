  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document domain(\"snakenetmetalradio.com\") {\r\n\r\na, a font { text-decoration: none !important;\r\n\tcolor: #FF9900 !important;\r\n}\r\n\r\na:hover, a font:hover {\r\n    color: red !important;\r\n}\r\n\r\n\r\ntable[width=\"700\"] a[href*=\"profile.asp\"], a[href*=\"profile.asp\"] font, .spnMessageText  a[href*=\"profile.asp\"], td   a[title*=\"View Member\"] {\r\n    color: /*[[un_color]]*/ !important;\r\n}\r\n\r\ntable[width=\"700\"] a[href*=\"profile.asp\"]:hover, a[href*=\"profile.asp\"] font:hover, .spnMessageText  a[href*=\"profile.asp\"]:hover, td   a[title*=\"View Member\"]:hover {\r\n    color:red !important;\r\n}\r\n\r\n\r\n.spnMessageText font[size=\"1\"]{\r\n    font-style: italic !important;\r\n}\r\n\r\ntable[width=\"95%\"] td {\r\n    padding:4px !important; \r\n}\r\n\r\ntable[width=\"725\"] td , table[width=\"700\"][align=\"center\"] td {\r\n    padding:4px !important;\r\n    vertical-align:middle !important;\r\n}   \r\n\r\ntable[cellspacing=\"10\"]  {\r\n vertical-align:top !important;\r\n}\r\n\r\n}",
