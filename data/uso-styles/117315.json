  "style_tags": [],
  "css": "@-moz-document domain(\"minecraftservers.org\") {\r\n\r\n.online {background: /*[[Online]]*/ !important}\r\n.offline {background: /*[[Offline]]*/ !important}\r\n\r\n/*Removes Ads*/\r\n.second-banner, .google-ad {\r\n\tdisplay: none !important;\r\n}\r\n\r\n/*Navbar*/\r\n.nav-banner {\r\n    background: /*[[Nav]]*/ !important;\r\n    opacity: .8 !important;\r\n    position: fixed !important;\r\n    width: 100% !important;\r\n    z-index: 100 !important;\r\n    \r\n}\r\n\r\n.brand-banner {padding-top: 50px !important;}\r\n\r\n/*Body Background*/\r\n#main, #header, .brand-banner, .second-banner, .page-server, body {\r\n\tbackground: /*[[Page Background]]*/ !important;\r\n}\r\n\r\n/*Blocks*/\r\n#footer, .serverlist thead tr th, .server-ip, .copy-action, .button, .section-head, .tags a, .vote, .version a {\r\n\tbackground: /*[[Blocks]]*/ !important;\r\n    color: #fff !important;\r\n}\r\n\r\n/*Text Color*/\r\nh1, h2, h3, h4, h5, h6, p, a, .count {\r\n\tcolor: /*[[Text Color]]*/ !important;\r\n}\r\n\r\n/*Copy Button*/\r\n.copy-action:hover, .zeroclipboard-is-hover {\r\n\tbackground: /*[[Copy Button Hover]]*/ !important;\r\n}\r\n\r\n/*Bottom Page Navigation*/\r\n.current-page, .pagination li:not(.current-page):hover {\r\n\tbackground: /*[[PageNav Button Hover]]*/ !important;\r\n}\r\n\r\n/*Bottom Page Navigation*/\r\n.pagination li:not(.current-page) {\r\n\tbackground: /*[[PageNav Button]]*/ !important;\r\n}\r\n\r\n.vote:hover {\r\n\tbackground: #1c1c1c !important;\r\n}\r\n\r\n.tab-content, .server-info td {\r\n\tbackground: #333 !important;\r\n    border: 1px solid #242424 !important;\r\n    color: #fafafa !important;\r\n}\r\n\r\n.section-head {\r\n\tborder: 1px solid #242424 !important;\r\n}\r\n\r\n.tab-link-active, .tab-links li:hover {\r\n\tborder-bottom: 6px solid #1c1c1c !important;\r\n}\r\n\r\n#field-container, .content-area, .flash {\r\n\tbackground: #484848 !important;\r\n    border: 1px solid #1c1c1c !important;\r\n    color: #fafafa !important;\r\n}\r\n\r\n.sponsored-servers {padding-bottom: 216px !important}\r\n\r\n#sponsored {\r\n\tpadding-bottom: 40px;\r\n}}",
