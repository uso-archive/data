  "css": "@-moz-document domain(lydsy.com)\r\n{\r\n  \r\nbody{\r\n    margin-left: 15%;\r\n    margin-right: 15%;\r\n    background-image: url('/*[[bgp]]*/');\r\n    background-attachment: fixed;\r\n    background-position: center top;\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    line-height:2;\r\n}\r\ndiv.content{\r\n  opacity: 0.8;\r\n  background-color:#e4f0f8;\r\n  border-radius: 10px;\r\n\r\n  padding-top:10px;\r\n  padding-bottom: 10px;\r\n}\r\nimg{\r\n  transition: all 0.2s;\r\n  opacity: 1;\r\n}\r\nimg:hover{\r\n  transform: scale(1.1,1.1);\r\n}\r\nspan.sampledata{\r\n  background-color: #ffffff00;\r\n  line-height:0.8;\r\n}\r\n\r\nh2{\r\n  color:/*[[titlecol]]*/;\r\n  font-size: 25;\r\n  margin-top: 30px;\r\n  margin-bottom: 5px;\r\n}\r\nspan.green{\r\n  color:/*[[titlecol]]*/;\r\n}\r\nspan.red{\r\n  color:/*[[titlecol]]*/;\r\n}\r\na{\r\n    font-size: 18;\r\n}\r\na:link{\r\n    color: /*[[linkcol]]*/;\r\n}\r\na:visited{\r\n    color: /*[[linkcol]]*/;\r\n}\r\na{\r\n  transition: color 0.3s;\r\n}\r\na:hover{\r\n  color: /*[[selcol]]*/;\r\n}\r\nth{\r\n  opacity: 1;\r\n}\r\ntr.evenrow{\r\n  background-color: /*[[evencol]]*/df;\r\n}\r\ntr.oddrow{\r\n  background-color: /*[[oddcol]]*/df;\r\n}\r\ntextarea{\r\n  background-color: #ffffffaa;\r\n  width: 70%;\r\n}\r\ndiv.notice{\r\n  opacity: /*[[opa]]*/;\r\n}\r\ninput{\r\n  opacity: /*[[opa]]*/;\r\n}\r\nselect{\r\n  opacity: /*[[opa]]*/;\r\n}\r\ncenter{\r\n  color:/*[[titlecol]]*/;\r\n}\r\n  \r\n}",
