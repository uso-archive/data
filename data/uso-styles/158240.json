  "css": "@-moz-document domain(\"anilist.co\") {\r\n/* Created by Bicker (https://anilist.co/user/bicker/)\r\n * Last updated 16-02-2019\r\n */\r\n\r\n/*[[theme]]*/\r\n\r\n/*[[color]]*/\r\n\r\n/* Navbar */\r\n/*[[nav]]*/\r\n\r\n/* Home */\r\n/*[[feedoptions]]*/\r\n\r\n/* Activity */\r\n.activity-feed .activity-entry {\r\n    margin-bottom: 15px;\r\n}\r\n.activity-text .text {\r\n\tborder-left: solid 7px rgba(var(--color-blue));\r\n}\r\n.activity-message .text {\r\n\tborder-left: solid 7px rgba(var(--color-green));\r\n}\r\n.reply-wrap .activity-replies {\r\n\tmargin: 1px 20px 0 20px;\r\n}\r\n.reply-wrap .activity-replies .reply {\r\n\tbackground: rgba(var(--color-foreground-alt));\r\n\tborder-left: 4px solid;\r\n\tborder-radius: 0;\r\n\tmargin-bottom: 1px;\r\n\tpadding: 9px;\r\n\tpadding-bottom: 1px;\r\n}\r\n.reply-wrap .activity-replies .reply:nth-child(2n) {\r\n    border-color: rgba(var(--color-blue));\r\n}\r\n.reply-wrap .activity-replies .reply p {\r\n\tmargin-top: 8px;\r\n\tmargin-bottom: 8px;\r\n}\r\n.reply-wrap .activity-replies .activity-edit .markdown-editor {\r\n\tborder-radius: 0;\r\n\tmargin: 0 0 1px 0;\r\n}\r\n.reply-wrap .activity-replies .activity-edit .input {\r\n\tborder-radius: 0;\r\n\tmargin-bottom: 0;\r\n}\r\n.reply-wrap .activity-replies .activity-edit h2 {\r\n\tmargin-top: 10px;\r\n}\r\n.reply-wrap .activity-replies .activity-edit .preview {\r\n\tborder-radius: 5px;\r\n}\r\n.reply-wrap .activity-replies .activity-edit .actions {\r\n\tmargin: 15px 0 0 0;\r\n}\r\n\r\n/* Banner */\r\n.header-wrap .banner {\r\n\tmargin-top: 0px;\r\n}\r\n.header-wrap .banner .shadow {\r\n\tdisplay: none;\r\n}\r\n.header-wrap .banner .name {\r\n\tbackground: rgba(var(--color-blue));\r\n\tborder-radius: 5px;\r\n\tpadding: 6px 15px;\r\n\tmargin: 16px 15px;\r\n}\r\n\r\n/*[[profilecolor]]*/\r\n\r\n/* Drop down menus */\r\n.quick-search .el-select .el-input .el-input__inner,\r\n.quick-search .el-select .el-input.is-focus .el-input__inner,\r\n.el-select-dropdown,\r\n.el-dropdown-menu,\r\n.el-dropdown-menu__item--divided::before {\r\n\tbackground: rgba(var(--color-foreground-blue));\r\n}\r\n.el-select-dropdown__item.hover,\r\n.el-select-dropdown__item:hover {\r\n\tbackground: rgba(var(--color-blue));\r\n}\r\n.el-dropdown-menu__item--divided {\r\n\tborder-color: rgba(var(--color-foreground));\r\n}\r\n.el-select-group__wrap:not(:last-of-type)::after {\r\n\tbackground: rgba(var(--color-background));\r\n}\r\n.el-popper[x-placement^=\"bottom\"] .popper__arrow,\r\n.el-popper[x-placement^=\"bottom\"] .popper__arrow::after {\r\n\tborder-bottom-color: rgba(var(--color-foreground-blue));\r\n}\r\n.el-popper[x-placement^=\"top\"] .popper__arrow,\r\n.el-popper[x-placement^=\"top\"] .popper__arrow::after {\r\n\tborder-top-color: rgba(var(--color-foreground-blue));\r\n}\r\n\r\n/* Forum */\r\n.comment-wrap {\r\n    border-left: 7px solid rgba(var(--color-blue));\r\n}\r\n.comment-wrap .child.odd {\r\n    border-left: 7px solid;\r\n}\r\n\r\n/* Other */\r\n.categories .category {\r\n\tbackground: rgba(var(--color-background)) !important;\r\n\tcolor: rgba(var(--color-blue));\r\n\tborder-radius: 5px;\r\n}\r\n.categories .category:hover {\r\n\tcolor: rgba(var(--color-blue));\r\n}\r\n.media .media-card {\r\n\tbox-shadow: none;\r\n}\r\n.user-page-unscoped.blue .progress .bar,\r\n.user-page-unscoped.purple .progress .bar,\r\n.user-page-unscoped.green .progress .bar,\r\n.user-page-unscoped.orange .progress .bar,\r\n.user-page-unscoped.red .progress .bar,\r\n.user-page-unscoped.pink .progress .bar,\r\n.user-page-unscoped.gray .progress .bar,\r\n.user-page-unscoped.custom-color .progress .bar::after {\r\n    background: rgba(var(--color-blue));\r\n}\r\n}",
