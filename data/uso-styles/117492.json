  "css": "@-moz-document url-prefix(\"https://plus.googleapis.com\"), domain(\"youtube.com\"), regexp(\"https://apis.google.com/.*/widget/render/comments?.*&first_party_property=YOUTUBE.*\"), regexp(\"https://plus.google.com/.*/widget/render/comments?.*\"), regexp(\"https://plus.google.com/.*/notifications/frame?.*\")\r\n{\r\n  .ytp-progress-bar-container\r\n  {\r\n   visibility: /*[[clean1]]*/ !important;\r\n  }\r\n\r\n\r\n  .ytp-chrome-controls\r\n  {\r\n   visibility: /*[[clean2]]*/ !important;\r\n  }\r\n\r\n  /*.ytp-chrome-bottom\r\n  {\r\n   visibility: hidden \r\n  }*/\r\n\r\n/* Suggestion by \"heubergen\": Fix shadow gradiant.*/\r\n\r\n\t.ytp-gradient-bottom\r\n\t{\r\n\t\tz-index: 0;\r\n\t}\r\n  \r\n/* Remove channel logo. */\r\n\t.annotation.iv-branding .branding-img-container \r\n  \t{\r\n\t\tdisplay: /*[[clean3]]*/ !important;\r\n\t}\r\n  \r\n  /*Remove fullscreen clutter*/\r\n.ytp-big-mode .ytp-title-text, .ytp-chrome-top-buttons{\r\n    visibility: hidden !important;\r\n\t}\r\n} ",
