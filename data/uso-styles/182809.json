  "css": "@-moz-document url-prefix(\"https://2b2t.io/\") {\r\n    body,.highcharts-background {\r\n        fill: /*[[background]]*/;\r\n    \tbackground-color:  /*[[background]]*/;\r\n    \tcolor: /*[[text]]*/;\r\n\t}\r\n\r\n  \t.highcharts-series-0 .highcharts-graph {\r\n      \tstroke:  /*[[que]]*/;\r\n    }\r\n  \r\n\t.highcharts-color-1 .highcharts-graph {\r\n    \tstroke:  /*[[total]]*/;\r\n\t}\r\n  \r\n    .highcharts-series-2 .highcharts-graph {\r\n    \tstroke:  /*[[prio-que]]*/;\r\n    }\r\n\r\n\t.highcharts-axis-labels text {\r\n    \tfill: /*[[text]]*/ !important;\r\n\t}\r\n\r\n\t.highcharts-legend-item text {\r\n    \tfill: /*[[text]]*/ !important;\r\n\t}\r\n\r\n\t.highcharts-axis text {\r\n    \tfill: /*[[text]]*/ !important;\r\n\t}\r\n\r\n\t.highcharts-credits {\r\n    \tfill: /*[[text]]*/ !important;\r\n\t}\r\n  \r\n   .highcharts-yaxis-grid .highcharts-grid-line {\r\n   \t\tstroke:  /*[[chart-lines]]*/;\r\n   }\r\n\r\n   .highcharts-xaxis .highcharts-axis-line, .highcharts-xaxis .highcharts-tick {\r\n   \t\tstroke:  /*[[chart-lines]]*/;\r\n   }\r\n}",
