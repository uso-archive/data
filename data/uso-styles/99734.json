  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n@-moz-document domain(mangareader.net)\r\n{\r\n#top_navigation, .secnav-first, .secnav, .secnav-last\r\n{\r\n\tdisplay: /*[[menu]]*/ !important;\r\n}\r\n.logo\r\n{\r\n\tdisplay: /*[[logo]]*/ !important;\r\n}\r\n#admaintop, .c7, .c9, .pageSkinLeft, .pageSkinRight\r\n{\r\n\tdisplay: /*[[front]]*/ !important;\r\n}\r\n#adtop, #adfooter, .content-r-ad, .content-l-ad\r\n{\r\n\tdisplay: /*[[ads]]*/ !important;\r\n}\r\n#mangainfo\r\n{\r\n\tdisplay: /*[[name]]*/ !important;\r\n}\r\n#topchapter\r\n{\r\n\tdisplay: /*[[nav]]*/ !important;\r\n}\r\n.zoombottom, #onlinemanga, #tfollowme\r\n{\r\n\tdisplay: none !important;\r\n}\r\n#bottomchapter, #wrapper_footer\r\n{\r\n\tdisplay: /*[[bottom]]*/ !important;\r\n}\r\n.aftersearch\r\n{\r\n\tdisplay: /*[[search]]*/ !important;\r\n}\r\n.top, #topchapter\r\n{\r\n\twidth: 800px !important;\r\n}\r\n.zoomtop\r\n{\r\n\tmargin-bottom: 5px !important;\r\n}\r\n.top, #wrapper_header, #topchapter\r\n{\r\n\theight: auto !important;\r\n\tborder: none !important;\r\n\tmargin: 0 auto !important;\r\n}\r\n.top, #topchapter, .zoomtop, .zoombottom, #bottomchapter, #wrapper_footer, #adtop, #adfooter, .content-r-ad, .content-l-ad\r\n{\r\n\tdisplay: /*[[read]]*/ !important;\r\n}\r\n#imgholder\r\n{\r\n\tposition: /*[[border]]*/ !important;\r\n\tleft: 0px !important;\r\n\ttop: 0px !important;\r\n}\r\n#img\r\n{\r\n\twidth: /*[[larg]]*/px !important;\r\n\theight: /*[[haut]]*/px !important;\t\r\n}\r\n.imageinside, .latesthotimages > .imageinside > a > img\r\n{\r\n\theight: 100px !important;\r\n\twidth: 80px !important;\r\n\tmargin-left: auto !important;\r\n\tmargin-right: auto !important;\r\n}\r\n.latesthotimages\r\n{\r\n\theight: 140px !important;\r\n}\r\n#latestchapters, .updates, .c6\r\n{\r\n\twidth: 400px !important;\r\n}\r\n#wrapper_body\r\n{\r\n\twidth: 800px !important;\r\n}\r\n.c3\r\n{\r\n\tposition: relative !important;\r\n\tbottom: 100px !important;\r\n}\r\n#mangainfo\r\n\t{\r\n\t\tfont-size: 8px !important;\r\n\t\tposition: absolute !important;\r\n\t\tmargin-top: 3px !important;\r\n\t\tmargin-left: 115px !important;\r\n\t\toverflow: hidden !important;\r\n\t\twidth: 400px !important;\r\n\t\twhite-space: nowrap !important;\r\n\t}\r\n#chapterMenu\r\n\t{\r\n\t\twidth: 108px !important;\r\n\t}\r\n#sec_navigation\r\n\t{\r\n\t\twidth: 225px !important;\r\n\t}\t\r\n.aftersearch\r\n\t{\r\n\t\tmargin-left: 0px !important;\r\n\t}\r\n#sec_navigation > ul\r\n\t{\r\n\t\tpadding-left: 0px !important;\r\n\t}\r\n.ac_results\r\n\t{\r\n\t\twidth: 222px !important;\r\n\t}\r\n.ac_results > ul\r\n\t{\r\n\t\toverflow: hidden !important;\r\n\t}\r\n.ac_results > ul > li > div\r\n\t{\r\n\t\twidth: auto !important;\r\n\t}\r\n}",
