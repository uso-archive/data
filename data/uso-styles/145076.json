  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n/*\r\n\r\n  Force compact for Google News New Design since June 2017\r\n\r\n  Author: backy0175\r\n  Date: 2017/07/09\r\n\r\n*/\r\n\r\n@-moz-document domain(\"news.google.com\") {\r\n\r\n/*[[OneColumn]]*/\r\n  \r\n  \r\n/*[[HideThumbnail]]*/\r\n  \r\n\r\n/*[[ThumbnailSize]]*/\r\n  \r\n  /* adjust paddings and margins */\r\n  .lPV2Xe \r\n  {padding: 8px !important;\r\n  padding-top: 0px !important;}\r\n  \r\n  .fkWPz,\r\n  .M1Uqc,\r\n  .zaeuPd\r\n  {margin-top: 8px !important;}\r\n\r\n\r\n/*[[HideRelatedCoverag]]*/\r\n\r\n\r\n  /* resize menu */\r\n  /* .gb_me */\r\n  .gb_le\r\n  {width: 16% !important;}\r\n  .zQTmif\r\n  {margin-left: 15% !important;}\r\n  .uyYuVb \r\n  {font-size: 12px !important;}\r\n  .v4IxVd\r\n  {width: 100% !important}\r\n\r\n\r\n  /* Adjust Margins */\r\n  .M1Uqc,\r\n  .O0WRkf,\r\n  .zaeuPd,\r\n  .oyJxcb\r\n  {margin-top: 4px !important;}\r\n  \r\n  .tgGNL\r\n  {margin-bottom: 4px !important;\r\n  margin-top: 8px !important;}\r\n  .WyeMbd\r\n  {margin-bottom: 16px !important;}\r\n  .lPV2Xe\r\n  {padding-bottom: 4px !important;}\r\n  \r\n\r\n  /* Adjust Font Size */\r\n  .adH5zf\r\n  {font-size: 20px !important;}\r\n  \r\n  /*[[FontSizeBody]]*/\r\n \r\n  /* 2017/10/12 - fix menu width issue */\r\n  .gb_dc:not(.gb_9b)\r\n  {width: 16% !important;}\r\n  \r\n  .JQZRxb\r\n  {width: 84% !important;}\r\n\r\n}",
