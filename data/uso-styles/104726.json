  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document domain(\"jsbin.com\") {\r\n  .CodeMirror-code {background: black !important}\r\n  #bin .editbox .CodeMirror {background:black}\r\n  .label.menu span strong a {color:white;text-shadow:none}\r\n  .label.open .name > strong {background:#333;border-color:#333}\r\n  .dropdownmenu, .dropdowncontent {background:#333;border-color:#333}\r\n  .dropdownmenu a:hover, .button:hover, a.hover {background:#444}\r\n  .dropdownmenu a, .button, a {text-shadow:none;color:white}\r\n  .dropdownmenu .small {color:#ccc}\r\n  .buttons {background:#333}\r\n  #control, .control {background:transparent !important}\r\n  .open .button-dropdown, .open .button-dropdown:hover, .open .button-open, .open .button-open:hover {background:#444;border-color:#444}\r\n  a[data-pro]:after, .pro-required {background:#555;border-color:#555}\r\n  hr {border-color:#555}\r\n  .button.group.hasContent, .button.group.active {background:#555 !important}\r\n  .dropdownmenu a:hover, .button:hover, a.hover {background:#444;border-color:#444}\r\n  .panel:before {background:#555}\r\n  .label {background:#000;color:#fff;line-height:26px}\r\n  .label .name {color:#fff}\r\n  .resize {border-color:#333 !important}\r\n  .card header {background:#444;color:#fff;height:28px}\r\n  .card {border-color:#444}\r\n  \r\n  /*log*/\r\n  .console-log-head {color:#fff;background:#444;border-color:#444}\r\n  .console-log {background:#333;color:#fff}\r\n\r\n  /*menu*/\r\n  #toppanel {background:#555 !important;color:#fff !important}\r\n  .toppanel-wrapper a {color:#ccc !important}\r\n  #enableUniversalEditorLabel {color:#ccc !important}\r\n  .toppanel-wrapper .toppanel-no-pro a {background:#666 !important;color:#fff !important}\r\n  .toppanel-love {border-color:#666 !important}\r\n  .toppanel-button {border-color:#666 !important}\r\n  \r\n  .CodeMirror-code pre {color:white}\r\n  .cm-s-jsbin span.cm-tag  {color:/*[[cm-tag]]*/ !important}\r\n  .cm-s-jsbin span.cm-property {color:/*[[cm-property]]*/ !important}\r\n  .cm-s-jsbin span.cm-keyword {color:/*[[cm-keyword]]*/ !important}\r\n  .cm-s-jsbin span.cm-builtin {color:/*[[cm-builtin]]*/ !important}\r\n  .cm-s-jsbin span.cm-variable {color:/*[[cm-variable]]*/ !important}\r\n  .cm-s-jsbin span.cm-number {color:/*[[cm-number]]*/ !important}\r\n  .cm-s-jsbin span.cm-atom {color:/*[[cm-atom]]*/ !important}\r\n  .cm-s-jsbin span.cm-def {color:/*[[cm-tag]]*/ !important}\r\n  .cm-s-jsbin span.cm-variable-3 {color:/*[[cm-variable3]]*/ !important}\r\n  .cm-s-jsbin span.cm-operator {color:/*[[cm-operator]]*/ !important}\r\n  .cm-s-jsbin span.cm-variable-2 {color:/*[[cm-variable2]]*/ !important}\r\n  .cm-s-jsbin span.cm-attribute {color:/*[[cm-attribute]]*/ !important}\r\n  .cm-s-jsbin span.cm-meta {color:/*[[cm-meta]]*/ !important}\r\n  .cm-s-jsbin span.cm-comment {color:/*[[cm-comment]]*/ !important}\r\n  .CodeMirror-activeline-background.CodeMirror-linebackground {background:/*[[cm-active]]*/ !important}\r\n  .CodeMirror div.CodeMirror-cursor {border-color:/*[[cm-cursor]]*/}\r\n  ::selection {background:#333 !important}\r\n  ::-moz-selection {background:#333 !important}\r\n}",
