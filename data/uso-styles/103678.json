  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document domain(\"forum.projanmo.com\") {\r\n    body { -webkit-font-smoothing: antialiased !important; \r\n        text-shadow: 1px 1px 1px rgba(0,0,0,0.08) !important; \r\n    }\r\n    .brd-page { font: 65% \"/*[[font]]*/\", SolaimanLipi, AponaLohit, Verdana, Helvetica, Arial, sans-serif !important;\r\n        letter-spacing: -.5px !important;\r\n    } * {font-size: /*[[fsize]]*/;}\r\n    .brd samp, .brd code, .brd pre, .brd option, .brd optgroup, .brd input, .brd select, .brd textarea, .brd td, .brd th {\r\n        font-family:  \"/*[[font]]*/\", SolaimanLipi,AponaLohit,Verdana,Helvetica,Arial,sans-serif !important;\r\n    }\r\n    span {\r\n        letter-spacing: 0px;\r\n    }\r\n    samp, code, pre, option, optgroup, input, select, textarea, td, th {\r\n        font-family:  \"/*[[font]]*/\", SolaimanLipi,AponaLohit,Verdana,Helvetica,Arial,sans-serif !important;\r\n    }\r\n}",
