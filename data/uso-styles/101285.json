  "css": "@-moz-document url-prefix(\"http://mazmorra.com.ar/chat\"), url-prefix(\"http://www.mazmorra.com.ar/chat\") {\r\n.userinfo                               {\r\n                                            background-color: /*[[fondouserinfo]]*/!important;\r\n                                            color: /*[[frenteuserinfo]]*/!important;\r\n                                        }\r\n\r\n.userinfo .desc,\r\n.num                                    {\r\n                                            color: /*[[highlight_pv]]*/!important\r\n                                        }\r\n\r\n.bodycontainer,\r\n.pvbody                                 {\r\n                                            background: /*[[fondo]]*/!important;\r\n                                            color: /*[[frente]]*/!important;\r\n                                        }\r\n\r\n.join                                   {\r\n                                            background: /*[[fondo_jp]]*/!important;\r\n                                            color: /*[[join]]*/!important;\r\n                                        }\r\n\r\n.lobbycontainer,\r\n.chatinfo,\r\n.chatexcerpt,\r\n.chatroomscontainer                     {\r\n                                            background: /*[[fondo_lobby]]*/!important;\r\n                                            color: /*[[frente_lobby]]*/!important;\r\n                                        }\r\n\r\n.message p img:not(.emoji)              {\r\n                                            opacity: 0.2!important\r\n                                        }\r\n\r\n.message p img:not(.emoji):hover        {\r\n                                            opacity: 1.0!important\r\n                                        }\r\n\r\n.part                                   {\r\n                                            background: /*[[fondo_jp]]*/!important;\r\n                                            color: /*[[part]]*/!important;\r\n                                        }\r\n\r\n.roomuserlist                           {\r\n                                            color: /*[[nicks]]*/!important\r\n                                        }\r\n\r\n.roomuserlist .selected,\r\n.roomuserlist .selected .usernamecontainer\r\n                                        {\r\n                                            background: /*[[fondo_nicks_sel]]*/!important;\r\n                                            color: /*[[nicks_sel]]*/!important;\r\n                                        }\r\n\r\n.roomuserlist .usernamecontainer:hover  {\r\n                                            background: /*[[fondo_nicks_sel]]*/!important\r\n                                        }\r\n\r\n.roomuserlist,\r\n.usernamecontainer,\r\n.usercontainer,\r\n.timestamp,\r\n.usermention,\r\n.roomuserlist ul li                     {\r\n                                            background: /*[[fondo_nicks]]*/!important\r\n                                        }\r\n\r\n.textbox                                {\r\n                                            background: /*[[borde_textbox]]*/!important\r\n                                        }\r\n\r\n.textbox input                          {\r\n                                            background: /*[[fondo_textbox]]*/!important;\r\n                                            color: /*[[frente_textbox]]*/!important;\r\n                                        }\r\n\r\n.timestamp                              {\r\n                                            color: /*[[timestamp]]*/!important\r\n                                        }\r\n\r\n.usermention                            {\r\n                                            color: /*[[mencion]]*/!important\r\n                                        }\r\n}",
