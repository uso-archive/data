  "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document domain(\"iwakuroleplay.com\") {\r\n    /* Regular Users. */\r\n    .username--style2 {\r\n        color: /*[[userColour]]*/ !important;\r\n    }\r\n\r\n    /* Administrators. */\r\n    .username--style3 {\r\n        color: /*[[adminColour]]*/ !important;\r\n    }\r\n\r\n    .userBanner.ribbonAdmin {\r\n        background-color: /*[[adminColour]]*/ !important;\r\n        border-color: /*[[adminColour]]*/ !important;\r\n    }\r\n\r\n    /* Security Department. */\r\n    .username--style4 {\r\n        color: /*[[securityColour]]*/ !important;\r\n    }\r\n\r\n    .userBanner.ribbonSecurity {\r\n        background-color: /*[[securityColour]]*/ !important;\r\n        border-color: /*[[securityColour]]*/ !important;\r\n    }\r\n\r\n    /* Content Development. */\r\n    .username--style39 {\r\n        color: /*[[contentColour]]*/ !important;\r\n    }\r\n\r\n    .userBanner.ribbonContent {\r\n        background-color: /*[[contentColour]]*/ !important;\r\n        border-color: /*[[contentColour]]*/ !important;\r\n    }\r\n\r\n    /* Community Maintenance. */\r\n    .username--style31 {\r\n        color: /*[[communityColour]]*/ !important;\r\n    }\r\n\r\n    .userBanner.ribbonCommunity {\r\n        background-color: /*[[communityColour]]*/ !important;\r\n        border-color: /*[[communityColour]]*/ !important;\r\n    }\r\n\r\n    /* Donating Members. */\r\n    .username--style14 {\r\n        color: /*[[donorColour]]*/ !important;\r\n    }\r\n\r\n    .userBanner.ribbonDonor {\r\n        background-color: /*[[donorColour]]*/ !important;\r\n        border-color: /*[[donorColour]]*/ !important;\r\n    }\r\n\r\n    /* Community Volunteers. */\r\n    .username--style16 {\r\n        color: /*[[communityColour]]*/ !important;;\r\n    }\r\n\r\n    .userBanner.ribbonVolunteer {\r\n        background-color: /*[[communityColour]]*/ !important;\r\n        border-color: /*[[communityColour]]*/ !important;\r\n    }\r\n\r\n    /* New Users. */\r\n    .username--style30 {\r\n        color: /*[[juniorColour]]*/ !important;\r\n    }\r\n}",
