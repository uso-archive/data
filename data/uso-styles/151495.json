  "css": "@-moz-document domain(\"discordapp.com\") {\r\na[class*=\"imageWrapper\"] {\r\n    border-radius: 0;\r\n    overflow: hidden;\r\n    height: auto !important;\r\n    width: auto !important;\r\n    position: unset;\r\n}\r\n\r\na[class*=\"imageWrapper\"]:hover {\r\n    text-decoration: underline;\r\n}\r\n\r\na[class*=\"imageWrapper\"] img {\r\n    border-radius: 3px;\r\n    position: unset;\r\n}\r\n\r\n.theme-light a[class*=\"imageWrapper\"] {\r\n    color: #737f8d;\r\n}\r\n\r\n.theme-dark a[class*=\"imageWrapper\"] {\r\n    color: hsla(0,0%,100%,.7);\r\n}\r\n\r\ndiv[class*=\"containerCozy\"] > a[class*=\"imageWrapper\"]:not([class*=\"embedImage\"])/*[[ignore-default]]*/:after {\r\n    content: attr(href);\r\n    font-family: monospace, monospace;\r\n    font-size: 1em;\r\n    margin-left: -77ch;\r\n    margin-bottom: 2px;\r\n    display: block;\r\n    white-space: nowrap;\r\n}\r\n}",
