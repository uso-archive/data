  "css": "@-moz-document domain(\"youtube.com\") {\r\n/* \r\n * Name: YouTube - Super Mario Kart progress bar video player theme\r\n * Title: Super Mario Kart progress bar video player theme\r\n * Author:  Crystallis\r\n * Version: 0.2\r\n *\r\n */\r\n\r\n.html5-scrubber-button, .ytp-scrubber-button {\r\n\tbackground: url(\"/*[[drivers]]*/\") ! important;\r\n\tbackground-repeat: no-repeat ! important;\r\n\twidth: 31px ! important;\r\n\theight: 33px ! important;\r\n\tborder: none ! important;\r\n\tmargin-left: -16px ! important;\r\n\tmargin-top: -19px ! important;\r\n}\r\n}",
