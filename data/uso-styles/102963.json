  "css": "@-moz-document domain(\"forum.blockland.us\") {\r\n\r\nbody {\r\n\t/*[[width]]*/\r\n\tbackground-color: /*[[bg]]*/ !important;\r\n\tmargin:0 auto !important;\r\n\tcolor: #222;\r\n}\r\n\r\nbody > table > tbody > tr > td {\r\n\tbackground: none !important;\r\n}\r\n\r\nbody > table > tbody > tr {\r\n\tbackground: none !important;\r\n}\r\n\r\n.tborder[cellpadding='0'] {\r\n\tmargin-top: 10px;\r\n\tbackground-color: #fff;\r\n\tborder: #444 1px solid;\r\n}\r\n\r\ndiv.smalltext {\r\n\tmargin-top:16px !important;\r\n}\r\n\r\n.catbg {\r\n\tbackground: #546faa none !important;\r\n}\r\n  \r\ninput {\r\n\tborder-width: 1px;\r\n\tbox-shadow: 1px 1px 0px rgba(0,0,0,0.2);\r\n\tpadding: 2px;\r\n\toutline: none;\r\n}\r\n\r\ntextarea {\r\n\tborder: 1px solid #555; \r\n}\r\n\r\ninput:focus {\r\n\tbackground-color: #bcccdb;\r\n}\r\n\r\nimg {\r\n\tmax-width: 95%;\r\n}\r\n\r\n.windowbg {\r\n  box-shadow: inset 1px 1px 1px 0px rgba(255,255,255,0.6);\r\n}\r\n\r\n}",
