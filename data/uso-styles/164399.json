  "css": "@-moz-document domain('www.youtube.com') {\r\n/* Search Box */\r\ndiv#container.ytd-searchbox {\r\n  border-radius: 10px 0 0 10px !important;\r\n  height: 38px;\r\n  border: none !important;\r\n  background: #0000002e !important;\r\n  box-shadow: none !important;\r\n}\r\n::placeholder {\r\n  color: #fffc;\r\n}\r\ninput[type=text] {\r\n  color: white !important;\r\n}\r\n/* Search icon div */\r\nbutton#search-icon-legacy {\r\n  border-radius: 0 10px 10px 0 !important;\r\n  border: none !important;\r\n  box-shadow: none !important;\r\n  background: #0000002e !important;\r\n}\r\n/* Magnifying glass icon */\r\nbutton#search-icon-legacy path {\r\n  color: white !important;\r\n}\r\n  \r\n/* Navbar */\r\ndiv#container.style-scope.ytd-masthead {\r\n  background-image: /*[[Gradient Theme]]*/\r\n}\r\n\r\n/* Other icons in Navbar */\r\ndiv#buttons.style-scope.ytd-masthead path {\r\n  color: #ffffffd6;\r\n}\r\n  \r\n  /* YouTube icon */\r\n#youtube-paths path {\r\n    fill: black !important;\r\n}\r\n\r\n/* Sandwich menu icon */\r\n#guide-icon path {\r\n    fill: black !important;\r\n}\r\n}",
