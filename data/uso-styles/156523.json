  "css": "/* ez pz 2 code */\r\n\r\nbody {\r\n    -webkit-animation: fadein /*[[BTime]]*/s !important;\r\n    -moz-animation: fadein /*[[BTime]]*/s;\r\n    -ms-animation: fadein /*[[BTime]]*/s;\r\n    -o-animation: fadein /*[[BTime]]*/s;\r\n    animation: fadein /*[[BTime]]*/s;\r\n}\r\nbody {\r\n    -webkit-animation: blur /*[[BTime]]*/s !important;\r\n    -moz-animation: blur /*[[BTime]]*/s !important;\r\n    -ms-animation: blur /*[[BTime]]*/s !important;\r\n    -o-animation: blur /*[[BTime]]*/s !important;\r\n    animation: blur /*[[BTime]]*/s !important;\r\n}\r\n@keyframes blur {\r\n    from {\r\n        filter: blur(/*[[BSize]]*/px);\r\n    }\r\n    to {\r\n        filter: blur(0px)\r\n    }\r\n}",
