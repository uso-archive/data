  "css": "@-moz-document url-prefix(http://www.eldarya.)\r\n{\r\n\r\n/*ИКОНКА*/\r\n#pet-headerPortrait #pet-headerPortrait-base {\r\n   background: url(\"/*[[css1]]*/\") no-repeat !important;\r\n   -webkit-filter: hue-rotate(0deg);\r\n   filter: hue-rotate(0deg);\r\n}  \r\n\r\n/*В ПРОФИЛЕ ГГ*/\r\n#pet-portrait > img[src*=\"/static/img/pet/icon/\"] {display : none !important ;}\r\n#player-display-pet {\r\n    height:  /*[[css5]]*/px!important ;\r\n    width:  /*[[css6]]*/px!important ;\r\n}  #player-display-pet > img[src*=\"/static/img/pet/mood/profile/\"] {opacity : 0 !important ; }\r\n   #player-display-pet {\r\n    background : url(\"/*[[css2]]*/\") center no-repeat !important ;\r\n    background-size: contain !important;\r\n} \r\n/*НА СТРАНИЦЕ РАЗВЕДКИ*/ \r\n   #pet-portrait {\r\n    background : url(\"/*[[css2]]*/\"), url(\"/*[[css3]]*/\") !important ;\r\n    background-size: contain !important;\r\n    background-position: bottom !important;\r\n    background-repeat: no-repeat !important;\r\n    height : 450px !important ;\r\n    width: 450px !important ;\r\n}   \r\n  \r\n/*ПОРОДА ФАМИЛЬЯРА*/\r\n #pet-info-racegender:before {\r\n    content : \"/*[[css4]]*/\" !important ;\r\n    font-size : 15px !important ;\r\n    line-height : 10px !important ;\r\n}  \r\n#pet-info-racegender {\r\n    font-size : 0 !important ;\r\n    margin-top : 5px ;\r\n}  \r\n\r\n\r\n\r\n\r\n/*ИМЯ*/\r\n\r\n#pet-info-name {\r\n    font-size: 0px;\r\n}\r\n\r\n#pet-info-name:before {\r\n    content: \"/*[[font]]*/\" !important;\r\n    font-size: 20px !important;\r\n    line-height: 16px !important;\r\n}\r\n\r\n\r\n/*ЕДА*/\r\n#pet-info-food > img {\r\n  background : url(\"/*[[css8]]*/\") !important ;\r\n  background-size : 72px 72px !important ;\r\n  margin-top : 0px !important ;\r\n  padding-left : 72px !important ;\r\n  padding-top : 72px !important ;\r\n  width : 0 !important ;\r\n  height : 0 !important ;    \r\n  border-radius: 50px;\r\n  border: 1px solid;\r\n} \r\n#pet-info-food {\r\n  font-size : 0 !important;}    \r\n#pet-info-food > span {\r\n  font-size : 15px !important;\r\n}\r\n}",
