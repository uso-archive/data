  "style": {
    "css": "@-moz-document domain(\"medium.com\") {\r\n.postArticle--full .sectionLayout--insetColumn {\r\n    max-width: calc(100% - /*[[margins]]*/px * 2);\r\n    padding-left: /*[[margins]]*/px;\r\n    padding-right: /*[[margins]]*/px;\r\n}\r\n.postArticle-content .graf--p,\r\n.graf--li {\r\n    font-size: 16px;\r\n}\r\n.postMetaHeader,\r\n.js-postShareWidget,\r\n.paragraphControls,\r\n.responsesStreamWrapper,\r\n.postActions,\r\n::-webkit-scrollbar {\r\n    display: none;\r\n}\r\n::selection {\r\n  background: #9BFFB3;\r\n}\r\n::-moz-selection {\r\n  background: #9BFFB3;\r\n}\r\n}",
