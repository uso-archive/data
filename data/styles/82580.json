  "style": {
    "css": "@-moz-document domain('www.evernote.com') {\r\n    body > div > div:last-child > div > div:nth-child(3) > div > div:nth-child(3) > div > div:first-child > div > div:nth-child(2) {\r\n        /* Replace to any width you want */\r\n        width: /*[[nlwidth]]*/ !important;\r\n    }\r\n\r\n    body > div > div:last-child > div > div:nth-child(3) > div > div:nth-child(3) > div > div:first-child > div > div:last-child {\r\n        /* Replace on width of previous style */\r\n        left: /*[[nlwidth]]*/ !important;\r\n    }\r\n\r\n    .GLMQCVEBEY { width: auto !important; }\r\n    .noteSnippetTitle { width: auto !important; }\r\n}",
