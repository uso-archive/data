  "style": {
    "css": "@-moz-document domain(\"twitch.tv\")\r\n{\r\nspan.chat-author__display-name[data-a-user=\"/*[[user1]]*/\"]{\r\n    color: /*[[color1]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user2]]*/\"]{\r\n    color: /*[[color2]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user3]]*/\"]{\r\n    color: /*[[color3]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user4]]*/\"]{\r\n    color: /*[[color4]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user5]]*/\"]{\r\n    color: /*[[color5]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user6]]*/\"]{\r\n    color: /*[[color6]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user7]]*/\"]{\r\n    color: /*[[color7]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user8]]*/\"]{\r\n    color: /*[[color8]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user9]]*/\"]{\r\n    color: /*[[color9]]*/!important;\r\n}\r\nspan.chat-author__display-name[data-a-user=\"/*[[user10]]*/\"]{\r\n    color: /*[[color10]]*/!important;\r\n}\r\n}",
