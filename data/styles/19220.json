  "style": {
    "css": "@namespace url(http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul);\r\n\r\n\r\n.tabbrowser-arrowscrollbox > .scrollbutton-up,\r\n.tabbrowser-arrowscrollbox > .scrollbutton-down,\r\n.tabs-alltabs-button,\r\n.tabs-newtab-button,\r\n.tabbrowser-tab {\r\n    -moz-appearance:button!important;\r\n    margin:1px 0!important; padding:0 3px!important;}\r\n\r\n.tabs-bottom{display:none!important;}\r\n\r\n.tabbrowser-tab * {margin:-4px 0!important;}\r\n.tabs-alltabs-button {padding:0!important; border:0px!important;}\r\n\r\n.tabs-alltabs-button .toolbarbutton-menu-dropmarker {margin: 0 -2px -2px 0!important;}\r\n\r\n.tabbrowser-arrowscrollbox > .scrollbutton-up .toolbarbutton-icon {margin:0 3px 0 2px!important;}\r\n.tabbrowser-arrowscrollbox > .scrollbutton-down .toolbarbutton-icon {margin:0 2px 0 3px!important;}\r\n\r\n.tabbrowser-arrowscrollbox > .scrollbutton-up:active .toolbarbutton-icon{margin:-1px 4px 2px 1px!important;}\r\n.tabbrowser-arrowscrollbox > .scrollbutton-down:active .toolbarbutton-icon{margin:-1px 3px 2px 2px!important;}\r\n\r\n.tab-close-button{-moz-appearance:none!important; margin:-4px 0!important;}\r\n",
