    "css": "@-moz-document url(\"https://iceposeidon.live/\") {\r\nbody {\r\n    /*[[background]]*/\r\n    overflow-y: hidden;\r\n}\r\n\r\n.channels {\r\n    border: 0px !important;\r\n}\r\n\r\n.channel:nth-child(odd):not(:hover) {\r\n    background-color: rgba(0,0,0,0) !important;\r\n}\r\n\r\n.channel:hover {\r\n    background-color: /*[[hlc]]*/14 !important;\r\n    border-radius: 2px !important;\r\n}\r\n\r\n.channel:active {\r\n    background-color: /*[[hlc]]*/26 !important;\r\n}\r\n\r\n.channel.online.youtube, .channel.online.twitch, .channel.online.dlive {\r\n    animation: rainbow-text 0.66s infinite linear;\r\n}\r\n\r\n@keyframes rainbow-text {\r\n0% {color: #ff0000;}\r\n17% {color: #ffff00;}\r\n33% {color: #00ff00;}\r\n50% {color: #00ffff;}\r\n67% {color: #0000ff;}\r\n83% {color: #ff00ff;}\r\n100% {color: #ff0000;}\r\n}\r\n\r\n.header, #footer, .channel:not(.online) {\r\n    visibility: hidden !important;\r\n}\r\n\r\n.status {\r\n    color: /*[[vcc]]*/ !important;\r\n}\r\n}",
